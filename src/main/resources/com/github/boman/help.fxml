<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane fx:id="root" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.boman.HelpController">
   <children>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="30.0">
         <children>
            <Label fx:id="wLabel" text="W: MOVE UP" textFill="WHITE" />
            <Label fx:id="aLabel" layoutX="10.0" layoutY="10.0" text="A: MOVE LEFT" textFill="WHITE" />
            <Label fx:id="sLabel" layoutX="10.0" layoutY="10.0" text="S: MOVE DOWN" textFill="WHITE" />
            <Label fx:id="dLabel" layoutX="10.0" layoutY="26.0" text="D: MOVE RIGHT" textFill="WHITE" />
            <Label fx:id="spaceLabel" text="SPACE: BOMB" textFill="WHITE" />
            <Button fx:id="returnButton" mnemonicParsing="false" onAction="#returnMenu" text="Return" />
         </children>
      </VBox>
   </children>
</StackPane>
